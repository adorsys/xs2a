{
    "extends": "../tslint.json",
    "rules": {
        "directive-selector": [
            true,
            "attribute",
            "app",
            "camelCase"
        ],
        "component-selector": [
            true,
            "element",
            "app",
            "kebab-case"
        ],
        "whitespace": [
          true,
          "check-module"
        ],
        "quotemark": [
          true,
          "single"
        ]
    }
}
