<databaseChangeLog xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" logicalFilePath="db.changelog-1.0.xml"
                   xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">

    <changeSet author="amu@adorsys.com.ua" id="2020-04-9-1">
        <comment>
            Change request_date_time and creation_timestamp type columns in consent table.
        </comment>

        <modifyDataType tableName="consent" columnName="request_date_time" newDataType="DATETIME"/>
        <modifyDataType tableName="consent" columnName="creation_timestamp" newDataType="DATETIME"/>

    </changeSet>

</databaseChangeLog>
